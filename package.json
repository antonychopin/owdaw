{
  "name": "owdaw",
  "version": "0.0.1",
  "description": "Open Web Digital Audio Workstation",
  "main": "src/index.tsx",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "CONFIG=dev webpack-dev-server --inline --watch-poll --colors --host 0.0.0.0 --port 1523",
    "dev:https": "CONFIG=dev webpack-dev-server --inline --watch-poll --colors --https --host 0.0.0.0 --port 1524",
    "build": "CONFIG=prod webpack --progress --config webpack.config.js",
    "build:minor": "CONFIG=prod BUILD=minor webpack --progress --config webpack.config.js",
    "build:major": "CONFIG=prod BUILD=major webpack --progress --config webpack.config.js",
    "commit": "version=`node -p \"require('./version').v()\"` && git add . && git commit -m \"$version $m\" && git push origin $b",
    "commit:master": "b=master npm run commit",
    "clear:dist": "rm -rf dist/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antonychopin/owdaw.git"
  },
  "author": "Limbo Sounds",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antonychopin/owdaw/issues"
  },
  "homepage": "https://github.com/antonychopin/owdaw#readme",
  "browserslist": [
    "> 1%", 
    "last 2 versions"
  ],
  "devDependencies": {
    "@types/express": "^4.17.0",
